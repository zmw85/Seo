@{
    ViewData["Title"] = "Home Page";
}

<h2>Google Search Result</h2>
<div class="row">
    <div class="col-xs-12">
        <form class="form-inline">
            <div class="form-group">
                <label for="exampleInputName2">Keyword:</label>
                <input type="text" class="form-control" id="txtKeyword" placeholder="Keyword" value="Online Title Search">
            </div>
            <div class="form-group">
                <label for="exampleInputEmail2">Url Prefix:</label>
                <input type="text" class="form-control" id="txtUrlPrefix" placeholder="Url Prefix" value="www.infotrack.com">
            </div>
            <button type="button" class="btn btn-primary" id="btnSearch">Search</button>
        </form>
    </div>
</div>

<div id="content"></div>



@section Scripts
{
    <script src="@Url.Content("~/jsx/index.jsx")"></script>
}